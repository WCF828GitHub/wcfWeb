<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.wcfweb.mapper.UserMapper">
    <select id="checkUserByNameAndPassWord" parameterType="com.example.wcfweb.entity.Users" resultType="com.example.wcfweb.entity.Users">
        select * from users where name=#{name} and passWord=#{passWord}
    </select>
</mapper>
